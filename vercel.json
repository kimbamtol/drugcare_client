{
    "version": 2,
    "builds": [
        {
            "src": "app/**/*.jsx",
            "use": "@vercel/next"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app/page.js"
        }
    ]
}
